<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>check_Company_Name</fullName>
    <active>true</active>
    <errorConditionFormula>AND(IsConverted,

Company  =  Account_Reference__r.Name
)</errorConditionFormula>
    <errorMessage>Cannot Quick convert, please check Company Name matches Account Reference Name</errorMessage>
</ValidationRule>
