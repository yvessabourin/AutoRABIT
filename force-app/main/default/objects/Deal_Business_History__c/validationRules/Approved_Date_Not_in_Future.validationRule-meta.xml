<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Approved_Date_Not_in_Future</fullName>
    <active>true</active>
    <errorConditionFormula>DATEVALUE(Approved_Date__c) &gt; TODAY()</errorConditionFormula>
    <errorDisplayField>Approved_Date__c</errorDisplayField>
    <errorMessage>Approval date cannot be in the future</errorMessage>
</ValidationRule>
