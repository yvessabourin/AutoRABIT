<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Village_Specific_Validation_rule</fullName>
    <active>false</active>
    <errorConditionFormula>AND (
ISBLANK(TEXT( Village_Specific__c )),
TEXT(  Visit_Source__c  )==&apos;Village_Specific&apos;

)</errorConditionFormula>
    <errorDisplayField>Village_Specific__c</errorDisplayField>
    <errorMessage>If Visit Source is Membership then Village Specific Field is Mandatory.</errorMessage>
</ValidationRule>
