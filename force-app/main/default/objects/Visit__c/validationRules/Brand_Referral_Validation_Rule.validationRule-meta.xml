<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Brand_Referral_Validation_Rule</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
ISBLANK(  Brand_Referral__c  ),
TEXT( Visit_Source__c )==&apos;Brand_Referral&apos;
)</errorConditionFormula>
    <errorDisplayField>Brand_Referral__c</errorDisplayField>
    <errorMessage>If Visit Source is Brand Referral then Brand Referral Field is Mandatory.</errorMessage>
</ValidationRule>
