<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Next_Financial_Year_End__c</fullName>
    <externalId>false</externalId>
    <formula>IF( Ye0__c  &gt;  TODAY() ,  Ye0__c ,  Ye1__c )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Calculated next financial year end date based on the date today and the 1st Financial Year end date</inlineHelpText>
    <label>Next Financial Year End</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Date</type>
</CustomField>
