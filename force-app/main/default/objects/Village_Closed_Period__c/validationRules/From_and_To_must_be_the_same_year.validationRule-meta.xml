<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>From_and_To_must_be_the_same_year</fullName>
    <active>true</active>
    <errorConditionFormula>YEAR( From__c )  &lt;&gt;  YEAR( To__c )</errorConditionFormula>
    <errorMessage>The from and To dates must be in the same year</errorMessage>
</ValidationRule>
