<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cannot_be_zero</fullName>
    <active>true</active>
    <errorConditionFormula>OR(FX_Rate_to_Group_CCY__c  &lt;= 0, ISBLANK( FX_Rate_to_Group_CCY__c) )</errorConditionFormula>
    <errorDisplayField>FX_Rate_to_Group_CCY__c</errorDisplayField>
    <errorMessage>The FX rate cannot be less than or equal to zero or blank</errorMessage>
</ValidationRule>
