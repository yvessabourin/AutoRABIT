<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Coniq_New_Review_Date_NOT_More_90_days</fullName>
    <active>true</active>
    <errorConditionFormula>Coniq_Next_Review_Date__c &gt;  (Coniq_Last_Review_Date__c + 90)</errorConditionFormula>
    <errorDisplayField>Coniq_Next_Review_Date__c</errorDisplayField>
    <errorMessage>The &quot;Next Review Date&quot; cannot be more than 90 after the &quot;Last Review Date&quot;</errorMessage>
</ValidationRule>
