<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Forecast_Year__c</fullName>
    <externalId>false</externalId>
    <formula>TEXT((YEAR( Contract_Start_Date__c )  +  Year__c ) - 1)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Based on the Start Date of the Contract and the Year this is the calculated actual year that forecasted sales and/or income will fall</inlineHelpText>
    <label>Forecast Year</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
