<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Leap_Year__c</fullName>
    <externalId>false</externalId>
    <formula>IF(MOD(YEAR( Contract_Start_Date__c ) + Year__c,4)=0,  CASE( Year__c ,1,1,2, 1,3,1,4,1,5,2,6,2,7,2,8,2,9,3,10,3,0), 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>If this is a leap year then this is the number of leap years since the start date of the contract and is used to correctly set the anniversary date</inlineHelpText>
    <label>Leap Year</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
