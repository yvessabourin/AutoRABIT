<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>yrsSinceCreated__c</fullName>
    <description>Number of full years since the record creation.
Years are based on 365 days.</description>
    <externalId>false</externalId>
    <formula>FLOOR((TODAY() -  DATEVALUE(CreatedDate ) ) / 365)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>yrsSinceCreated</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
