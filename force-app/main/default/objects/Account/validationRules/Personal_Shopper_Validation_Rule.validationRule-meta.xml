<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Personal_Shopper_Validation_Rule</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
ISBLANK(  Personal_Shopper__c  ),
TEXT(  Guest_Source__c  )==&apos;Personal_Shopper&apos;
)</errorConditionFormula>
    <errorDisplayField>Personal_Shopper__c</errorDisplayField>
    <errorMessage>If Guest Source is Personal Shopper then Personal Shopper Field is Mandatory.</errorMessage>
</ValidationRule>
