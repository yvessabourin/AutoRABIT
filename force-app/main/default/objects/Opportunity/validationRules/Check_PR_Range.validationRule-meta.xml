<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Check_PR_Range</fullName>
    <active>true</active>
    <errorConditionFormula>OR (Position_Rating__c  &lt; 0, Position_Rating__c  &gt; 10)</errorConditionFormula>
    <errorDisplayField>Position_Rating__c</errorDisplayField>
    <errorMessage>Position rating must be in the range 1-10</errorMessage>
</ValidationRule>
