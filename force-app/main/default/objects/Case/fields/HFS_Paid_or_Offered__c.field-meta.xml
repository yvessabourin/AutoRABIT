<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>HFS_Paid_or_Offered__c</fullName>
    <description>For reporting purposes, mapping the values for Payment Status to OFFERED or PAID</description>
    <externalId>false</externalId>
    <formula>IF(CONTAINS(UPPER(TEXT(Payment_Status__c)),&apos;FREE OF CHARGE&apos;),&apos;OFFERED&apos;,&apos;PAID&apos;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Paid or Offered</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
