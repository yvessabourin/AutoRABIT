<?xml version="1.0" encoding="UTF-8"?>
<Workflow xmlns="http://soap.sforce.com/2006/04/metadata">
    <outboundMessages>
        <fullName>ObmPromotionRedemption</fullName>
        <apiVersion>33.0</apiVersion>
        <endpointUrl>https://api.valueretail.com/sf/promotionRedemption/v1.0/ObmPromotionRedemption.asmx</endpointUrl>
        <fields>CreatedById</fields>
        <fields>CreatedDate</fields>
        <fields>Email_opt_in__c</fields>
        <fields>Guest_Email__c</fields>
        <fields>Guest_First_Name__c</fields>
        <fields>Guest_Last_Name__c</fields>
        <fields>Id</fields>
        <fields>IsDeleted</fields>
        <fields>LastModifiedById</fields>
        <fields>LastModifiedDate</fields>
        <fields>LastReferencedDate</fields>
        <fields>LastViewedDate</fields>
        <fields>Name</fields>
        <fields>Redeem_Date__c</fields>
        <fields>Redeemable_Campaign__c</fields>
        <fields>SystemModstamp</fields>
        <fields>Voucher_Code__c</fields>
        <fields>customer__c</fields>
        <includeSessionId>true</includeSessionId>
        <integrationUser>b2capi@valueretail.com</integrationUser>
        <name>ObmPromotionRedemption</name>
        <protected>false</protected>
        <useDeadLetterQueue>false</useDeadLetterQueue>
    </outboundMessages>
    <rules>
        <fullName>Sync Promotion Redemption with B2C Customer ODS</fullName>
        <actions>
            <name>ObmPromotionRedemption</name>
            <type>OutboundMessage</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Promotion_Redemption__c.CreatedDate</field>
            <operation>greaterThan</operation>
            <value>1/1/1900</value>
        </criteriaItems>
        <criteriaItems>
            <field>User.ProfileId</field>
            <operation>notEqual</operation>
            <value>B2C ODS API</value>
        </criteriaItems>
        <triggerType>onAllChanges</triggerType>
    </rules>
</Workflow>
