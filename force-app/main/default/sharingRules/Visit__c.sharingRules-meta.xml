<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingCriteriaRules>
        <fullName>Visits_shared_will_all</fullName>
        <accessLevel>Edit</accessLevel>
        <description>non VIP Visits, will be shared with everyone (giving effective view all data access, to all but VIP Visits)</description>
        <label>Visits shared will all</label>
        <sharedTo>
            <roleAndSubordinatesInternal>CEO</roleAndSubordinatesInternal>
        </sharedTo>
        <criteriaItems>
            <field>RecordTypeId</field>
            <operation>notEqual</operation>
            <value>VIP Visit</value>
        </criteriaItems>
    </sharingCriteriaRules>
